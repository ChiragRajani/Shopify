<%- include('partials/header') -%>
<div class="container">
    <br> <h3 id="head">Your Products</h5> <br>
  <div class="row text-center " style="display:flex; flex-wrap:wrap ;">
    <div class="row" id="rw">
      <% product.forEach(function(product){   %>
        <div id="th">
       
              <div class="column-sm"> 
               
                <div  class="img-thumbnail" id="thumb">
                      <img  src = "<%=  product.image %>"  width="250" height="140">
                     <div  class="caption">
                       <h3><%= product.name %></h3>
                       <a href="/product/<%= product._id%>" class="btn btn-dark">More Infos</a>
                    </div> 
                  </div>
                  <br>
              </div>
          
        </div>
      <%  }) ;  %>
    </div>
  </div>
</div>
<%- include('partials/footer') -%>